  <prescreen>
    <pre_media file="2.2.01_III_premedia_1.jpg"/>
    <pre_text sound="2.2.01_III_title.mp3">Morgens auf der Baustelle.</pre_text>
    <pre_text sound="2.2.01_I_pretext_1.mp3">Leschek Kowalski und Martina Tossini arbeiten heute zufällig auf der gleichen Baustelle. Sie nutzen die Gelegenheit und treffen sich ab und zu in den Pausen im Bauwagen.</pre_text>
  </prescreen>
  <instruction>
    <in_text style="bold" font="Verdana" size="14" color="white" sound="2.2.01_III_question_instruct.mp3">Klicken Sie bitte auf das leuchtende Lautsprechersymbol, hören Sie zu und ergänzen Sie den Text. Sie können sich einzelne Wörter wiederholen lassen indem Sie in der entsprechenden Lücke auf den Lautsprecher klicken.</in_text>
  </instruction>
  <questions>
    <questionpage>
      <q_text sound="2.2.01_III_question_instruct_part_I.mp3"/>
      <question>
        <answertext>
          <a_text>Leschek setzt sich in den</a_text>
          <gap answerid="2.2.01_III_1" style="TextInput_blue_22" length="10" sound="2.2.01_III_question_1.mp3"/>
          <a_text>. Er nimmt sich eine Zeitung und beginnt zu lesen.</a_text>
        </answertext>
        <answertext>
          <a_text>Der Sprecher der regierenden</a_text>
          <gap answerid="2.2.01_III_2" style="TextInput_blue_22" length="10" sound="2.2.01_III_question_2.mp3"/>
          <a_text>hat</a_text>
          <gap answerid="2.2.01_III_3" style="TextInput_blue_22" length="10" sound="2.2.01_III_question_3.mp3"/>
          <a_text>bekannt gemacht, dass die</a_text>
          <gap answerid="2.2.01_III_4" style="TextInput_blue_22" length="10" sound="2.2.01_III_question_4.mp3"/>
          <a_text>von Telefon- und Mailverbindungen von allen</a_text>
          <gap answerid="2.2.01_III_5" style="TextInput_blue_22" length="10" sound="2.2.01_III_question_5.mp3"/>
          <a_text>für eine</a_text>
          <gap answerid="2.2.01_III_6" style="TextInput_blue_22" length="10" sound="2.2.01_III_question_6.mp3"/>
          <a_text>von mehreren</a_text>
          <gap answerid="2.2.01_III_7" style="TextInput_blue_22" length="10" sound="2.2.01_III_question_7.mp3"/>
          <a_text>gespeichert</a_text>
          <gap answerid="2.2.01_III_8" style="TextInput_blue_22" length="10" sound="2.2.01_III_question_8.mp3"/>
          <a_text>.</a_text>
        </answertext>
        <answertext>
          <a_text>Das dient angeblich der Sicherheit.</a_text>
        </answertext>
      </question>
    </questionpage>
    <questionpage>
      <q_text sound="2.2.01_III_question_instruct_part_II.mp3"/>
      <question>
        <answertext>
          <a_text>Einige</a_text>
          <gap answerid="2.2.01_III_9" style="TextInput_blue_22" length="10" sound="2.2.01_III_question_9.mp3"/>
          <a_text>der Opposition sehen das anders. Sie sind überzeugt, dass</a_text>
          <gap answerid="2.2.01_III_10" style="TextInput_blue_22" length="10" sound="2.2.01_III_question_10.mp3"/>
          <a_text>Freiheit</a_text>
          <gap answerid="2.2.01_III_11" style="TextInput_blue_22" length="10" sound="2.2.01_III_question_11.mp3"/>
          <a_text>Sicherheit getauscht wird und dass ein Überwachungsstaat</a_text>
          <gap answerid="2.2.01_III_12" style="TextInput_blue_22" length="10" sound="2.2.01_III_question_12.mp3"/>
          <a_text>alle gefährlich ist. Was</a_text>
          <gap answerid="2.2.01_III_13" style="TextInput_blue_22" length="10" sound="2.2.01_III_question_13.mp3"/>
          <a_text>Sie?</a_text>
        </answertext>
      </question>
    </questionpage>
  </questions>
