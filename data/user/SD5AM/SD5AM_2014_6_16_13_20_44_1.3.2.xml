      <question>
        <answertext>
          <a_text>Im Büro schneit es immer.</a_text>
        </answertext>
        <choices>
          <choice answerid="1.3.2_1_1">Richtig.</choice>
          <choice answerid="1.3.2_1_2">Falsch.</choice>
        </choices>
      </question>
    </questionpage>
    <questionpage>
      <question>
        <answertext>
          <a_text>Ein Ei kostet zehn Euro.</a_text>
        </answertext>
        <choices>
          <choice answerid="1.3.2_2_1">Richtig.</choice>
          <choice answerid="1.3.2_2_2">Falsch.</choice>
        </choices>
      </question>
    </questionpage>
    <questionpage>
