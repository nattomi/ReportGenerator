<!--Instructions - Left side entry-->
<instruction>
   <!--Text-->
   <in_text sound="2.3.01_III_instruct_2.mp3">Und dies ist nun die Nachricht von Herrn Müller an Simones Kollegin Frau Scholz. &#13;Klicken Sie bitte auf das Lautsprechersymbol, hören Sie zu und ergänzen Sie die Buchstaben und Wörter in dem Text. Sie können sich einzelne Wörter wiederholen lassen, indem Sie in der entsprechenden Lücke auf den Lautsprecher klicken. Beachten Sie bitte besonders die Groß- und Kleinschreibung.</in_text>
   <!--Media-->
   <media file="2.3.01_III_premedia_1.png" />
</instruction>
<!--Questions - Right side entry-->
<questions>
  <!-- Page - partcollection for one question display-->
  <questionpage>
	   <question>
	    <!--<answertext sound="2.3.01_III_question_text_1.mp3"></answertext>-->
	      <answertext>
	      		<a_text>Datum:</a_text>
	      		<gap sound="2.3.01_III_question_1.mp3" answerid="2.3.01_III_1" style="TextInput_blue" length="17" />
	      </answertext>
	      <answertext sound="2.3.01_III_question_text_1.mp3">
	       		<a_text>Hallo Frau Scholz,</a_text>
	      </answertext>
	      <answertext>
	       	<a_text>Das</a_text>
			<gap sound="2.3.01_III_question_2.mp3" answerid="2.3.01_III_2" style="TextInput_blue" length="8" />
			<a_text>ihrer</a_text>
			<gap sound="2.3.01_III_question_3.mp3" answerid="2.3.01_III_3" style="TextInput_blue" length="2" />
			<a_text>ollegin ist</a_text>
			<gap sound="2.3.01_III_question_4.mp3" answerid="2.3.01_III_4" style="TextInput_blue" length="2" />
			<a_text>lötzlich krank geworden.</a_text>
			<a_text>Frau</a_text>
			<gap sound="2.3.01_III_question_5.mp3" answerid="2.3.01_III_5" style="TextInput_blue" length="2" />
			<a_text>ischer</a_text>
			<gap sound="2.3.01_III_question_6.mp3" answerid="2.3.01_III_6" style="TextInput_blue" length="5" />
			<a_text>daher nicht kommen.</a_text>
			<gap sound="2.3.01_III_question_7.mp3" answerid="2.3.01_III_7" style="TextInput_blue" length="5" />
			<a_text>säubern sie</a_text>
			<gap sound="2.3.01_III_question_8.mp3" answerid="2.3.01_III_8" style="TextInput_blue" length="5" />
			<a_text>Tafeln im 2. Stock. Leeren sie auch</a_text>
			<gap sound="2.3.01_III_question_9.mp3" answerid="2.3.01_III_9" style="TextInput_blue" length="6" />
			<a_text>Papierkörbe und wischen sie den</a_text>
			<gap sound="2.3.01_III_question_10.mp3" answerid="2.3.01_III_10" style="TextInput_blue" length="6" />
			<a_text>.</a_text>
	    <!--  </answertext>
	      <answertext>-->
			<a_text>Vielen Dank und einen schönen</a_text>
			<gap sound="2.3.01_III_question_11.mp3" answerid="2.3.01_III_11" style="TextInput_blue" length="2" />
			<a_text>ag.</a_text>
	      </answertext>
	      <answertext>
			<a_text>Walter Müller</a_text>
	     </answertext>
	   </question>
  </questionpage> 
</questions>
