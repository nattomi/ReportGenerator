<!--Instructions - Left side entry-->
<instruction>
   <!--Text-->
      <in_text sound="2.3.01_II_question_instruct_1.mp3">Anschließend schreibt sie noch eine Mail an ihren Arbeitgeber Herrn Müller.&#13;Klicken Sie bitte auf das Lautsprechersymbol, hören Sie zu und ergänzen Sie die Buchstaben und Wörter in dem Text. Sie können sich einzelne Wörter wiederholen lassen, indem Sie in der entsprechenden Lücke auf den Lautsprecher klicken. Beachten Sie bitte besonders die Groß- und Kleinschreibung.</in_text>
   <!--Media-->
   <media file="2.3.01_II_instruction_1.png" />
</instruction>
<!--Questions - Right side entry-->
<questions>
  <!-- Page - partcollection for one question display-->
  <questionpage> 
  	   <q_text sound="2.3.01_II_question_text_2.mp3"></q_text>
	   <question>
	      <answertext>
	      		<a_text>Lara hat Fieber</a_text>
	      		</answertext>
	      		<answertext>
	      		<a_text>Sehr geehrter Herr </a_text>
	      		<gap sound="2.3.01_II_question_1.mp3" answerid="2.3.01_II_1" style="TextInput_blue" length="2" /><a_text>üller,</a_text>
	      		</answertext>
	      		<answertext>
			<gap sound="2.3.01_II_question_2.mp3" answerid="2.3.01_II_2" style="TextInput_blue" length="2" /><a_text>eine</a_text>
			<gap sound="2.3.01_II_question_3.mp3" answerid="2.3.01_II_3" style="TextInput_blue" length="2" /><a_text>ochter Lara ist</a_text>
			<gap sound="2.3.01_II_question_4.mp3" answerid="2.3.01_II_4" style="TextInput_blue" length="2" /><a_text>eute krank. Ich kann daher leider nicht zur Arbeit</a_text>
			<gap sound="2.3.01_II_question_5.mp3" answerid="2.3.01_II_5" style="TextInput_blue" length="9" /><a_text>.</a_text>
			</answertext>
			<answertext>
			<gap sound="2.3.01_II_question_6.mp3" answerid="2.3.01_II_6" style="TextInput_blue" length="44"/><a_text>.</a_text>
			</answertext>
			<answertext>
			<a_text>Mit freundlichen Grüßen</a_text>
			</answertext>
			<answertext>
			<a_text>Simone Fischer</a_text>
	     </answertext>
	   </question>
  </questionpage> 
</questions>
